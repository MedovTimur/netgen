[package]
name = "{{project_name}}"
version = "0.1.0"
edition = "2021"

[dependencies]
anyhow = "1"
tokio = { version = "1", features = ["full"] }
axum = "0.7"
serde = { version = "1", features = ["derive"] }
{{#if tracing_enabled}}
tracing = "0.1"
tracing-subscriber = { version = "0.3", features = ["env-filter"] }
{{/if}}
{{#if db_enabled}}
sqlx = { version = "0.8", features = ["runtime-tokio-rustls", "postgres"] }
{{/if}}
